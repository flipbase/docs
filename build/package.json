{
  "name": "docs",
  "version": "0.1.0",
  "scripts": {
    "start": "./node_modules/.bin/gitbook serve ./",
    "setup": "./node_modules/.bin/gitbook install",
    "build": "./node_modules/.bin/gitbook build ./ ./build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "major": "./node_modules/.bin/standard-version --release-as major",
    "minor": "./node_modules/.bin/standard-version --release-as minor",
    "patch": "./node_modules/.bin/standard-version --release-as patch"
  },
  "devDependencies": {
    "standard-version": "^4.3.0",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2"
  },
  "dependencies": {
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-custom-favicon": "0.0.4",
    "gitbook-plugin-ga": "^1.0.1",
    "gitbook-plugin-insert-logo": "^0.1.5",
    "gitbook-plugin-toggle-chapters": "0.0.3",
    "gitbook-plugin-toggle-headers": "^0.1.0",
    "gitbook-plugin-versions": "^2.0.1"
  }
}
